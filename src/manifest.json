{
  "name": "chrome-kjs",
  "description": "Web开发辅助插件，集成二维码、常见的加密方法...",
  "version": "0.1.3",
  "manifest_version": 2,
  "default_locale": "zh_CN",
  "homepage_url": "https://chrome.google.com/webstore/detail/chrome-kjs/dffdfpjckicpnigcfjfkfooimeckmodm",
  "options_ui": {
      "page": "options.html",
      "open_in_tab": false,
      "chrome_style": true
  },
  "permissions": [
    "storage",
    "contextMenus",
    "tabs",
    "notifications",
    "cookies",
    "webRequest",
    "*://*/*",
    "webRequestBlocking",
    "webNavigation"
  ],

  "content_scripts": [{
       "matches": ["http://*/*", "https://*/*"],
       "js": ["statics/js/content.js"],
       "run_at": "document_end",
       "all_frames": true
  }],

  "web_accessible_resources":[
  ],

  "icons": {
      "16": "statics/images/favicon.png",
      "48": "statics/images/favicon.png",
      "128": "statics/images/favicon.png"
  },
  "background": {
      "page": "background.html"
  },


  "browser_action": {
    "default_icon": "statics/images/favicon.png",
    "default_popup": "popup.html"
  }
}
